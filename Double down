local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Double down free version", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "autofarm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "autofarm"
})

Tab:AddTextbox({
	Name = "gamepass ",
	Default = "put gamepass id here",
	TextDisappear = true,
	Callback = function(gamepass)
		getgenv().pass = gamepass
	end	  
})

Tab:AddTextbox({
	Name = "name game",
	Default = "SWORD FIGHT",
	TextDisappear = true,
	Callback = function(game)
		getgenv().games = game
	end	  
})

Tab:AddTextbox({
	Name = "put ticket ",
	Default = "20",
	TextDisappear = true,
	Callback = function(Option)
		getgenv().ticket = tonumber(Option)
	end	  
})

Tab:AddDropdown({
	Name = "asset type",
	Default = "GamePass",
	Options = {"Asset", "GamePass"},
	Callback = function(section)
		getgenv().asset = section
	end    
})

Tab:AddToggle({
	Name = "autofarm",
	Default = false,
	Callback = function(state)
        getgenv().autofarm = state
        while getgenv().autofarm do
            local args = {
                [1] = getgenv().games,
                [2] = getgenv().ticket,
                [3] = {
                    ["assetType"] = getgenv().asset,
                    ["assetId"] = getgenv().pass
                },
                [4] = true
            }
            game:GetService("ReplicatedStorage"):WaitForChild("RemoteCalls"):WaitForChild("GameSpecific"):WaitForChild("Tickets"):WaitForChild("CreateRoom"):InvokeServer(unpack(args))
            task.wait(40)
        end
	end    
})

Tab:AddButton({
	Name = "free robux only server",
	Callback = function()
        game:GetService("Players").LocalPlayer.DataSave.Common.FillOnStart.Gamepasses.RobuxServers.Value = true
        OrionLib:MakeNotification({Name = "Lukas Hub", Content = "Server Unlocked!", Time = 3})
  	end    
})

OrionLib:Init()
