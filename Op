local p = game.Players.LocalPlayer
local runService = game:GetService("RunService")
local vu = game:GetService("VirtualUser")

-- ANTI-AFK (pořádně silný)
p.Idled:Connect(function()
    vu:CaptureController()
    vu:ClickButton2(Vector2.new())
end)

-- TURBO CLICKER
spawn(function()
    while true do
        -- Místo jednoho kliknutí jich pošleme 5 najednou v jedné smyčce
        for i = 1, 5 do
            vu:Button1Down(Vector2.new(0, 0))
            vu:Button1Up(Vector2.new(0, 0))
        end
        task.wait() -- Nejkratší možná pauza
    end
end)

-- SKRÝVÁNÍ TABULKY 123
runService.RenderStepped:Connect(function()
    for _, v in pairs(p.PlayerGui:GetChildren()) do
        if v:IsA("ScreenGui") and (v.Name:find("123") or v.Name == "Check") then
            v.Enabled = false
        end
    end
end)
